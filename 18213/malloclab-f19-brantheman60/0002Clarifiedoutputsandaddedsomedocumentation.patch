diff --git a/macro-check.pl b/macro-check.pl
index 631de0f..bd4e1a8 100755
--- a/macro-check.pl
+++ b/macro-check.pl
@@ -45,6 +45,8 @@ $lineno = 1;
 $startlineno = 1;
 $line = "";
 
+$mcount = 0;
+
 while (<$infile>) {
     $nextline = $_;
     chomp($nextline);
@@ -62,7 +64,8 @@ while (<$infile>) {
 		$linemsg = "Lines $startlineno-$lineno contain";
 	    }
 	    if ($macname !~ /^$ok_prefix/) {
-		print "WARNING:$fwarning $linemsg disallowed definition of macro '$macname'\n";
+		print "ERROR:$fwarning $linemsg disallowed definition of macro '$macname'\n";
+		$mcount += 1;
 	    }
 	}
 	$line = "";
@@ -70,3 +73,8 @@ while (<$infile>) {
     }
     $lineno++;
 }
+
+if ($mcount > 0) {
+    print "FATAL: $mcount macro(s) detected.";
+    exit(1);
